{
    "name": "@pixy/svelte",
    "version": "0.0.1-alpha.6",
    "private": false,
    "description": "A free open-source library of precision-designed vectorized icons",
    "author": "Grange Louis",
    "license": "MIT",
    "homepage": "https://pixy.grangelouis.ch",
    "bugs": "https://github.com/l0uisgrange/pixy/issues",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/l0uisgrange/pixy.git"
    },
    "type": "module",
    "keywords": [
        "pixy",
        "svelte",
        "library",
        "icon",
        "icons",
        "svg",
        "lucide",
        "feather",
        "phosphore",
        "font awesome"
    ],
    "scripts": {
        "prebuild": "tsx scripts/generate.ts",
        "build": "vite build && npm run prepack",
        "preview": "vite preview",
        "prepare": "svelte-kit sync || echo ''",
        "prepack": "svelte-kit sync && svelte-package && publint",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
    },
    "files": [
        "dist",
        "!dist/**/*.test.*",
        "!dist/**/*.spec.*",
        "README.md",
        "package.json",
        "LICENSE"
    ],
    "sideEffects": [
        "**/*.css"
    ],
    "svelte": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "svelte": "./dist/index.js"
        }
    },
    "peerDependencies": {
        "svelte": "^5.0.0"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^6.0.0",
        "@sveltejs/kit": "^2.22.0",
        "@sveltejs/package": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^6.0.0",
        "publint": "^0.3.2",
        "svelte": "^5.0.0",
        "svelte-check": "^4.0.0",
        "typescript": "^5.0.0",
        "vite": "^7.0.4",
        "tsx": "^4.20.4",
        "@types/node": "^24.2.1"
    }
}
