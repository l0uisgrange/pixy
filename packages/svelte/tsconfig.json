{
	"compilerOptions": {
		"module": "ESNext",
		"target": "ESNext",
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"declaration": true,
		"noImplicitAny": false,
		"sourceMap": true,
		"strict": true,
		"outDir": "./dist",
		"moduleResolution": "bundler",
	}
}
